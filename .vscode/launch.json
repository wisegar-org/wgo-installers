  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell: Launch Current File",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": [
        "-oldversion",
        "11",
        "-newversion",
        "16",
        "-server",
        "localhost",
        "-username",
        "postgres",
        "-oldport",
        "5432",
        "-newport",
        "5434"
      ],
      "cwd": "${fileDirname}"
    }
  ]
}
